{
  "name": "deployhq-mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol (MCP) server for DeployHQ API integration",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
